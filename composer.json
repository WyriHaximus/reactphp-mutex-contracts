{
    "name": "wyrihaximus/react-mutex-contracts",
    "description": "Mutex locking contracts for ReactPHP projects",
    "license": "MIT",
    "require": {
        "php": "^8 || ^7.4",
        "react/promise": "^2.8"
    },
    "require-dev": {
        "wyrihaximus/async-test-utilities": "^3.3"
    },
    "config": {
        "platform": {
            "php": "7.4.7"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\React\\Mutex\\Contracts\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
